{
  "name": "kakidash-on-vscode",
  "displayName": "Mindmap vscode extension powered by kakidash",
  "icon": "icon.png",
  "publisher": "hiroooo000",
  "repository": "https://github.com/hiroooo000/kakidashOnVSCode.git",
  "description": "VSCode extension to view mindmaps using kakidash",
  "keywords": [
    "mindmap",
    "kakidash",
    "visualization",
    "diagram",
    "productivity",
    "brainstorming",
    "idea"
  ],
  "version": "0.0.5",
  "engines": {
    "vscode": "^1.96.0"
  },
  "packageManager": "pnpm@10.26.0",
  "activationEvents": [
    "onCustomEditor:kakidash.mindmap"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "kakidash.mindmap",
        "displayName": "Kakidash Mindmap",
        "selector": [
          {
            "filenamePattern": "*.{mm,mindmap}"
          }
        ]
      }
    ],
    "configuration": {
      "title": "Kakidash",
      "properties": {
        "kakidash.nodeWidth": {
          "type": "number",
          "default": 300,
          "description": "Maximum width of nodes in pixels. Set to -1 for unlimited width."
        },
        "kakidash.style.canvas.background": {
          "type": "string",
          "format": "color-hex",
          "default": "#fafafa",
          "description": "Background color of the canvas."
        },
        "kakidash.style.rootNode.background": {
          "type": "string",
          "format": "color-hex",
          "default": "#ffeeee",
          "description": "Background color of the root node."
        },
        "kakidash.style.rootNode.color": {
          "type": "string",
          "format": "color-hex",
          "default": "#333333",
          "description": "Text color of the root node."
        },
        "kakidash.style.rootNode.borderStyle": {
          "type": "string",
          "enum": [
            "solid",
            "dashed",
            "dotted",
            "double",
            "none"
          ],
          "default": "solid",
          "description": "Border style of the root node."
        },
        "kakidash.style.rootNode.borderWidth": {
          "type": "number",
          "default": 4,
          "description": "Border width of the root node (px)."
        },
        "kakidash.style.rootNode.borderColor": {
          "type": "string",
          "format": "color-hex",
          "default": "#FFD700",
          "description": "Border color of the root node."
        },
        "kakidash.style.childNode.background": {
          "type": "string",
          "format": "color-hex",
          "default": "#ffffff",
          "description": "Background color of the child nodes."
        },
        "kakidash.style.childNode.color": {
          "type": "string",
          "format": "color-hex",
          "default": "#555555",
          "description": "Text color of the child nodes."
        },
        "kakidash.style.childNode.borderStyle": {
          "type": "string",
          "enum": [
            "solid",
            "dashed",
            "dotted",
            "double",
            "none"
          ],
          "default": "dashed",
          "description": "Border style of the child nodes."
        },
        "kakidash.style.childNode.borderWidth": {
          "type": "number",
          "default": 2,
          "description": "Border width of the child nodes (px)."
        },
        "kakidash.style.childNode.borderColor": {
          "type": "string",
          "format": "color-hex",
          "default": "#0000FF",
          "description": "Border color of the child nodes."
        },
        "kakidash.style.connection.color": {
          "type": "string",
          "format": "color-hex",
          "default": "#FFA500",
          "description": "Color of the connections."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "build": "npm run compile",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "create-vsix": "vsce package --no-yarn --no-dependencies",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "jest",
    "turbo:build": "turbo run build",
    "turbo:test": "turbo run test",
    "turbo:lint": "turbo run lint",
    "turbo:package": "turbo run package",
    "turbo:ci": "turbo run build test lint"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/jest": "^30.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.19.28",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "@vscode/vsce": "^3.7.1",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "mocha": "^10.2.0",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "turbo": "^2.7.6",
    "typescript": "^5.1.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {
    "kakidash": "0.1.13"
  }
}