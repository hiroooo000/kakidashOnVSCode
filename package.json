{
  "name": "kakidash-on-vscode",
  "displayName": "Mindmap vscode extension powered by kakidash",
  "icon": "icon.png",
  "publisher": "hiroooo000",
  "repository": "https://github.com/hiroooo000/kakidashOnVSCode.git",
  "description": "VSCode extension to view mindmaps using kakidash",
  "keywords": [
    "mindmap",
    "kakidash",
    "visualization",
    "diagram",
    "productivity",
    "brainstorming",
    "idea"
  ],
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "activationEvents": [
    "onCustomEditor:kakidash.mindmap"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "kakidash.mindmap",
        "displayName": "Kakidash Mindmap",
        "selector": [
          {
            "filenamePattern": "*.{mm,mindmap}"
          }
        ]
      }
    ],
    "configuration": {
      "title": "Kakidash",
      "properties": {
        "kakidash.nodeWidth": {
          "type": "number",
          "default": 300,
          "description": "Maximum width of nodes in pixels. Set to -1 for unlimited width."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "create-vsix": "vsce package --no-yarn",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "jest"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/jest": "^30.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.19.28",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "@vscode/vsce": "^3.7.1",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "mocha": "^10.2.0",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.1.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {
    "kakidash": "^0.0.9"
  }
}